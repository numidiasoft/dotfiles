yeahconsole*term:urxvt
yeahconsole*aniDelay:0
yeahconsole*consoleHeight:23
yeahconsole*keyFull:None+F11
yeahconsole*toggleKey:None+F12
yeahconsole*keySmaller:ControlAlt+h
yeahconsole*keyBigger:ControlAlt+l
yeahconsole*metaSendsEscape:true
yeahconsole*restart:1

URxvt.buffered: true
URxvt.background: black
URxvt.foreground: white
URxvt.cursorColor: green
URxvt.underlineColor: yellow
URxvt.font: xft:Terminus:pixelsize=12:antialias=false
URxvt.boldFont: xft:Terminus:bold:pixelsize=12:antialias=false
URxvt.saveLines: 4096
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.scrollstyle: plain
URxvt.transparent: true
URxvt.tintColor: black
URxvt.shading: 32
URxvt.perl-ext-common: default,clipboard
